import java.util.*;

public class singlerow {
    public static void main(String[] args) {
        int i, num;
        Scanner sc = new Scanner(System.in);
        System.out.println("enter the number:");
        num = sc.nextInt();
        for (i = 1; i <= num; i++) {
            System.out.print(i + " ");
        }

    }

}
